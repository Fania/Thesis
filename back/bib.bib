@phdthesis{Westendorf2011,
abstract = {Creativity support tools gainedmuch interest in recent years. Companies claim creativity as their strength and interdisciplinary project teams work together for innovative outcomes. The growth of social networks together with the internet enhances a place independent collaboration. A common understanding of creativity is the development of something novel and useful. Supporting the development of creative ideas is a dificult task due to the fact that creativity can hardly be defined and varies on the field it occurs [5]. The analysis of creativity and knowledge is a challenging task. Knowledge Engineering [60] is a field with the aim to derive new knowledge from data and information. Data and Text Mining techniques are used to analyse huge amounts of data for interconnections and patterns. Knowledge about creativity has to be based on a model of computational creativity that bridges the gap between abstract theoretical models and techniques of implementation. Knowledge representation is essential for a consistent storage of gained knowledge and its reuse. Existing data and text mining techniques [59] [2] in the field of knowledge engineering leave a gap in the area of creativity and transliteracy. The main aim of the proposed research is the development of a creativity mining system based on Web 2.0 compliant models of computational creativity and knowl- edge representation. It adresses the features freshness, incompleteness, imprecision, inconsistency and trustfulness. The system is based on individual and commonly agreed system knowledge created by user interaction. A Self-Organizing Map [33] is used for storing and clustering information using similarity measures. This report will outline the motivation and objectives for the proposed creativity mining system. It will dis- cuss initial results and present a time scale for further development. Furthermore, it will review literature from the knowledge engineering and computational creativity domain.},
author = {Westendorf, Sascha},
booktitle = {Creativity},
file = {:G$\backslash$:/Documents/PhD/Reading/Bibliography/Westendorf - 2011 - Knowledge Engineering for Creativity.pdf:pdf;:G$\backslash$:/Documents/PhD/Reading/Bibliography/Westendorf - 2011 - Knowledge Engineering for Creativity(2).pdf:pdf;:G$\backslash$:/Documents/PhD/Reading/Bibliography/Westendorf - 2011 - Knowledge Engineering for Creativity(3).pdf:pdf},
school = {DMU},
title = {{Knowledge Engineering for Creativity}},
year = {2011}
}
