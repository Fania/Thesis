@phdthesis{Osinski2003,
abstract = {In this thesis we propose a description-oriented algorithm for clustering of results obtained from Web search engines called LINGO. The key idea of our method is to first discover meaningful cluster labels and then, based on the labels, determine the actual content of the groups. We show how the cluster label discovery can be accomplished with the use of the Latent Semantic Indexing technique. We also discuss several factors that influence the quality of cluster description, such as input data preprocessing and language identification. As part of this thesis we prepared an implementation of our algorithm within the framework of Carrot2. To evaluate the practical performance of our algorithm we propose an expert- based scheme for assessment of clustering results.},
author = {Osinski, Stanislaw},
file = {:G$\backslash$:/Documents/PhD/Reading/Bibliography/Osinski - 2003 - An Algorithm or Clustering of Web Search Results.pdf:pdf},
